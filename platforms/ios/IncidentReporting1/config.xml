<?xml version='1.0' encoding='utf-8'?>
<widget id="com.slalom.incident_reporting" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="PushNotification">
        <param name="ios-package" value="PushPlugin" />
    </feature>
    <preference name="BackupWebStorage" value="local" />
    <feature name="com.salesforce.oauth">
        <param name="ios-package" value="SalesforceOAuthPlugin" />
    </feature>
    <feature name="com.salesforce.sdkinfo">
        <param name="ios-package" value="SFSDKInfoPlugin" />
    </feature>
    <feature name="com.salesforce.sfaccountmanager">
        <param name="ios-package" value="SFAccountManagerPlugin" />
    </feature>
    <feature name="com.salesforce.smartstore">
        <param name="ios-package" value="SFSmartStorePlugin" />
    </feature>
    <feature name="com.salesforce.smartsync">
        <param name="ios-package" value="SFSmartSyncPlugin" />
    </feature>
    <feature name="NetworkStatus">
        <param name="ios-package" value="CDVConnection" />
    </feature>
    <feature name="SpinnerDialog">
        <param name="ios-package" value="CDVSpinnerDialog" />
    </feature>
    <feature name="StatusBar">
        <param name="ios-package" value="CDVStatusBar" />
        <param name="onload" value="true" />
    </feature>
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <feature name="Camera">
        <param name="ios-package" value="CDVCamera" />
    </feature>
    <preference name="CameraUsesGeolocation" value="false" />
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser" />
    </feature>
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <splash height="2048" src="www/img/hseIPadPortraitSplashx2.png" width="1536" />
    <splash height="1536" src="www/img/hseIPadLandscapeSplashx2.png" width="2048" />
    <splash height="768" src="www/img/hseIPadLandscapeSplash.png" width="1024" />
    <splash height="1024" src="www/img/hseIPadPortraitSplash.png" width="768" />
    <splash height="1136" src="www/img/hseIPhonePortraitSplash.png" width="640" />
    <name>IR-Demo</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="www/img/hseIcon.png" />
</widget>
